{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "eslint:recommended",
        "plugin:@typescript-eslint/recommended"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "valid-jsdoc": "off",
        "require-jsdoc": "off",
        "eol-last": "off",
        "no-tabs": "off",
        "max-len": "off",
        "no-inferrable-types": "false"
    }
}